forward RaceCountDown(playerid);
public RaceCountDown(playerid)
{
	new str[56];
	format(str, sizeof(str), "~y~Race Starts In: %i", countdown);
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		if(InRace[i] == 1)
		{
			if(countdown == 0)
			{
				if(RaceTrack == 1)
				{
					GameTextForPlayer(i, "~g~GO!!!", 1000, 3);
					TogglePlayerControllable(i, 1);
					KillTimer(RaceStartTimer);
					SetPlayerRaceCheckpoint(i, 0, 692.65887, -529.82501, 22.86493, 681.70038, -508.23352, 15.79699, 5);
					DillimoreCPT[i] = 1;
					Finishers = 1;
					format(str, sizeof(str), "Lap: %i/%i", rInfo[i][rCLap], RaceLaps);
					PlayerTextDrawSetString(i, rLapCount[i], str);   
					PlayerTextDrawShow(i, rLapCount[i]);
				}
				else if(RaceTrack == 2)
				{
					GameTextForPlayer(i, "~g~GO!!!", 1000, 3);
					TogglePlayerControllable(i, 1);
					KillTimer(RaceStartTimer);
					SetPlayerRaceCheckpoint(i, 0, 1293.8163, 258.8410, 18.9415, 1268.2251, 258.5081, 18.9200, 5);
					MontgomeryCPT[i] = 1;
					Finishers = 1;
					rInfo[i][rCLap] = 0;
					format(str, sizeof(str), "Lap: %i/%i", rInfo[i][rCLap], RaceLaps);
					PlayerTextDrawSetString(i, rLapCount[i], str);   
					PlayerTextDrawShow(i, rLapCount[i]);
				}
				else if(RaceTrack == 3)
				{
					GameTextForPlayer(i, "~g~GO!!!", 1000, 3);
					TogglePlayerControllable(i, 1);
					KillTimer(RaceStartTimer);
					SetPlayerRaceCheckpoint(i, 0, -2058.4956, -134.9543, 35.1039, -2049.3250, -167.9038, 35.1039, 5);
					GoKartSF1CPT[i] = 1;
					Finishers = 1;
					rInfo[i][rCLap] = 0;
					format(str, sizeof(str), "Lap: %i/%i", rInfo[i][rCLap], RaceLaps);
					PlayerTextDrawSetString(i, rLapCount[i], str);   
					PlayerTextDrawShow(i, rLapCount[i]);
				}
			}
			else
			{
				new variables[7];
				GameTextForPlayer(i, str, 1000, 3);
				if(countdown == 10)
				{
					if(RaceTrack == 1)
					{
						if(InRace[i] == 1)
						{
							SetPlayerPos(i, DillimoreGrid[rInfo[i][rGridPos]][0], DillimoreGrid[rInfo[i][rGridPos]][1], DillimoreGrid[rInfo[i][rGridPos]][2]+0.7); 
							SetVehiclePos(rInfo[i][rVehicleID], DillimoreGrid[rInfo[i][rGridPos]][0], DillimoreGrid[rInfo[i][rGridPos]][1], DillimoreGrid[rInfo[i][rGridPos]][2]);
							SetVehicleVirtualWorld(rInfo[i][rRaceCar], 101);
							SetPlayerVirtualWorld(i, 101);
							PutPlayerInVehicle(i, rInfo[i][rVehicleID], 0);
							SetVehicleZAngle(rInfo[i][rVehicleID], 88.6200);
							TogglePlayerControllable(i, 0);
							SetVehicleParamsEx(rInfo[i][rVehicleID] , VEHICLE_PARAMS_ON, variables[1], variables[2], variables[3], variables[4], variables[5], variables[6]);
							rInfo[i][rCLap] = 0;
						}
					}
					else if(RaceTrack == 2)
					{
						if(InRace[i] == 1)
						{
							SetPlayerPos(i, MontgomeryGrid[rInfo[i][rGridPos]][0], MontgomeryGrid[rInfo[i][rGridPos]][1], MontgomeryGrid[rInfo[i][rGridPos]][2]+0.7); 
							SetVehiclePos(rInfo[i][rVehicleID], MontgomeryGrid[rInfo[i][rGridPos]][0], MontgomeryGrid[rInfo[i][rGridPos]][1], MontgomeryGrid[rInfo[i][rGridPos]][2]);
							SetVehicleVirtualWorld(rInfo[i][rRaceCar], 102);
							SetPlayerVirtualWorld(i, 102);
							PutPlayerInVehicle(i, rInfo[i][rVehicleID], 0);
							SetVehicleZAngle(rInfo[i][rVehicleID], 155.5799);
							TogglePlayerControllable(i, 0);
							SetVehicleParamsEx(rInfo[i][rVehicleID] , VEHICLE_PARAMS_ON, variables[1], variables[2], variables[3], variables[4], variables[5], variables[6]);
							rInfo[i][rCLap] = 0;
						}
					}
					else if(RaceTrack == 3)
					{
						if(InRace[i] == 1)
						{
							SetPlayerPos(i, GoKartSFGrid[rInfo[i][rGridPos]][0], GoKartSFGrid[rInfo[i][rGridPos]][1], GoKartSFGrid[rInfo[i][rGridPos]][2]+0.7); 
							SetVehiclePos(rInfo[i][rVehicleID], GoKartSFGrid[rInfo[i][rGridPos]][0], GoKartSFGrid[rInfo[i][rGridPos]][1], GoKartSFGrid[rInfo[i][rGridPos]][2]);
							SetVehicleVirtualWorld(rInfo[i][rRaceCar], 103);
							SetPlayerVirtualWorld(i, 103);
							PutPlayerInVehicle(i, rInfo[i][rVehicleID], 0);
							SetVehicleZAngle(rInfo[i][rVehicleID], -90.0600);
							TogglePlayerControllable(i, 0);
							SetVehicleParamsEx(rInfo[i][rVehicleID] , VEHICLE_PARAMS_ON, variables[1], variables[2], variables[3], variables[4], variables[5], variables[6]);
							rInfo[i][rCLap] = 0;
						}
					}
				}
			}
		}
	}
	countdown--;
	return 1;
}

stock GetGridPos(playerid)
{
	for(new i = 0; i <= 15; i++)
	{
		if(GridPosTaken[i] == 1)
		{
			continue;
		}
		else
		{
			GridPosTaken[i] = 1;
			return i;
		}
	}
	return SendClientMessage(playerid, COLOR_GREY, "Race is currently full");
}

stock SendClientMessageToRacers(color, string[])
{
	foreach(Player, i)
	{
		if(InRace[i] == 1)
		{
			SendClientMessage(i, color, string);
		}
	}
	return 0;
}